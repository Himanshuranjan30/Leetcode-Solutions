class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int i=0;
        bool ans=false;
        while(i<bits.size()){
            if(bits[i]==1){           
                i+=2;
                ans=false;    
            }
            else{                        
                i+=1;
                ans=true;
            }
        }
        return ans;
    }
};
